{
  "name": "backend",
  "version": "0.0.1",
  "author": "Ismael Alves <cearaismael1997@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "dev": "cross-env ENVIROMENT=DEV nodemon --exec babel-node index.js",
    "test": "cross-env ENVIROMENT=TEST babel-node jest.startup.js --forceExit --detectOpenHandles --maxWorkers=1 --testTimeout=50000 --coverage",
    "secure-mode": "cross-env ENVIROMENT=TEST babel-node jest.startup.js --forceExit --detectOpenHandles --watchAll --maxWorkers=1 --testTimeout=50000",
    "doc": "apidoc -i doc/ -o src/public/documentacao",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -d -e DB_HOST=mongo -p 3000:3000 --name backend --network backend_default  backend",
    "docker:down": "docker stop backend",
    "docker:remove": "docker container rm backend"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.19.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "frameguard": "^3.1.0",
    "fs": "0.0.1-security",
    "googleapis": "^60.0.1",
    "helmet": "^3.23.3",
    "helmet-csp": "^2.10.0",
    "hide-powered-by": "^1.1.0",
    "hsts": "^2.2.0",
    "ienoopen": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.10",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.14",
    "prom-client": "^12.0.0",
    "rate-limiter-flexible": "^2.1.13",
    "response-time": "^2.3.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "speedtest-net": "^1.6.2",
    "uuid": "^3.4.0",
    "x-xss-protection": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/parser": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "babel-jest": "^26.6.1",
    "cross-env": "^7.0.2",
    "husky": "^3.1.0",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "supertest": "^4.0.2"
  }
}
